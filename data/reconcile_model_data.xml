<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="qr_sip_reconcile_model" model="account.reconcile.model">
            <field name="name">QR SIP</field>
            <field name="rule_type">invoice_matching</field>
            <field name="auto_reconcile" eval="True"/>
            <field name="match_nature">amount_received</field>
            <field name="allow_payment_tolerance" eval="True"/>
            <field name="payment_tolerance_param">0</field>
            <field name="payment_tolerance_type">percentage</field>
            <field name="match_same_currency" eval="False"/>
            <field name="match_text_location_label" eval="False"/>
            <field name="match_text_location_reference" eval="True"/>
            <field name="match_transaction_type">contains</field>
            <field name="match_transaction_type_param">QR SIP</field>
        </record>

    </data>
</odoo>