<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="get_sip_token_server_action" model="ir.actions.server">
        <field name="name">Get sip token</field>
        <field name="model_name">payment.provider</field>
        <field name="model_id" ref="payment.model_payment_provider"/>
        <field name="binding_model_id" ref="payment.model_payment_provider"/>
        <field name="state">code</field>
        <field name="code">
for record in records:
    if record.code == 'sip':
        record._get_sip_auth_token()
        </field>
    </record>

</odoo>