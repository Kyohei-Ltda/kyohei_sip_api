<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record model="ir.cron" id="cron_get_auth_token">
            <field name="name">SIP: Obtener token de autenticaci√≥n</field>
            <field name="model_id" ref="payment.model_payment_provider"/>
            <field name="state">code</field>
            <field name="code">model.cron_get_auth_token()</field>
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=4, minute=0, second=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

    </data>
</odoo>